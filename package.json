{
  "name": "returnsx",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "prisma generate && remix vite:build",
    "vercel-build": "prisma generate && remix vite:build",
    "dev": "shopify app dev",
    "config:link": "shopify app configure link",
    "generate": "shopify app generate",
